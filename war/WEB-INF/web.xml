<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>servlet.IndexServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>HomepageServlet</servlet-name>
    <servlet-class>servlet.HomepageServlet</servlet-class>
  </servlet>
     <servlet>
    <servlet-name>QuickBuildServlet</servlet-name>
    <servlet-class>servlet.QuickBuildServlet</servlet-class>
  </servlet>
 
  


  <!-- Servlet mappings -->
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>HomepageServlet</servlet-name>
    <url-pattern>/homepage</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
    <servlet-name>QuickBuildServlet</servlet-name>
    <url-pattern>/quickbuild</url-pattern>
  </servlet-mapping>

</web-app>
